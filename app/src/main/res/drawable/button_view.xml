<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

        <item android:state_pressed="false">
            <shape android:shape="rectangle">

                <size
                    android:height="84dp"/>
                <stroke
                    android:width="3dp"
                    android:color="@color/purple_700"/>

                <corners
                    android:radius="17dp"/>
                <padding
                    android:bottom="9dp"
                    android:top="5dp"
                    android:left="5dp"
                    android:right="5dp"/>
            </shape>
        </item>
    </selector>
